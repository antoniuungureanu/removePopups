{
  "manifest_version": 2,
  "name": "Remove popups",
  "version": "1.0",
 
  "description": "Removes general popups from pages",
 
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["remove popups.js"]
    }
  ],
 
  "permissions": [
    "webNavigation",
    "webRequest"
  ]
}